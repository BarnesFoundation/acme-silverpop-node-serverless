{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5", 
    "module": "commonjs",
    "lib": [
      "es2015"
    ],
    "outDir": "./dist",
    "rootDir": "./",
    "strict": false, 
    "moduleResolution": "node", 
    "baseUrl": "./", 
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@classes/*": [
        "src/app/classes/*"
      ],
      "@enums/*": [
        "src/app/enums/*"
      ],
      "@interfaces/*": [
        "src/app/interfaces/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }, 
    "esModuleInterop": true
  }
}