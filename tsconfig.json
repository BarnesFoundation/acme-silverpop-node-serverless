{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@classes/*": [
        "src/app/classes/*"
      ],
      "@enums/*": [
        "src/app/enums/*"
      ],
      "@interfaces/*": [
        "src/app/interfaces/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ]
}
